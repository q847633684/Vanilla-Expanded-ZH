  <!-- EN: No dedicated laboratory -->
  <VTE_NoDedicatedLaboratory.stages.No_dedicated_laboratory.label>没有专门的实验室</VTE_NoDedicatedLaboratory.stages.No_dedicated_laboratory.label>
