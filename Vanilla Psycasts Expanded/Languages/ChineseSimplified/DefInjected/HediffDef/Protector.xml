  <!-- EN: psychic healing -->
  <VPE_Healing.label>心灵治愈</VPE_Healing.label>
  <!-- EN: Psychically-induced healing boost. -->
  <VPE_Healing.description>灵能强化了治愈能力。</VPE_Healing.description>
  
  <!-- EN: psychic immunity -->
  <VPE_Immunity.label>心灵免疫</VPE_Immunity.label>
  <!-- EN: Psychically-induced immunity boost. -->
  <VPE_Immunity.description>灵能强化了免疫力。</VPE_Immunity.description>
  
  <!-- EN: psychic overshield -->
  <VPE_Overshield.label>护盾赋予</VPE_Overshield.label>
