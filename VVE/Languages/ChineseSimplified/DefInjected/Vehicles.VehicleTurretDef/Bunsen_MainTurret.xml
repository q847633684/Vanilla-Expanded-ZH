  <!-- EN: Fire Spew -->
  <VVE_Bunsen_MainTurret.fireModes.0.label>喷火</VVE_Bunsen_MainTurret.fireModes.0.label>
  <!-- EN: Select Target.\n\nFire for effect -->
  <VVE_Bunsen_MainTurret.gizmoDescription>选择目标。\n\n效力射击</VVE_Bunsen_MainTurret.gizmoDescription>
  
