  <VVE_Nauticals.symbolPacks.0.adjective>远航</VVE_Nauticals.symbolPacks.0.adjective>
  <!-- EN: Nauticals -->
  <VVE_Nauticals.symbolPacks.0.ideoName>航海</VVE_Nauticals.symbolPacks.0.ideoName>
