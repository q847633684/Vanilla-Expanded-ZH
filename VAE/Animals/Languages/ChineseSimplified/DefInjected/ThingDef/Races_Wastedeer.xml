  <!-- EN: teeth -->
  <VAEWaste_Wastedeer.tools.teeth.label>牙齿</VAEWaste_Wastedeer.tools.teeth.label>
  <!-- EN: head -->
  <VAEWaste_Wastedeer.tools.head.label>头</VAEWaste_Wastedeer.tools.head.label>
  
